        <af:resource type="javascript">
          function enforcePreventUserInput(evt) {
              var popup = AdfPage.PAGE.findComponentByAbsoluteId('page1:glassPane');
              if (popup != null) {
                  AdfPage.PAGE.addBusyStateListener(popup, handleBusyState);
                  evt.preventUserInput();
              }
          }

          function handleBusyState(evt) {
              var popup = AdfPage.PAGE.findComponentByAbsoluteId('page1:glassPane');
              if (popup != null) {
                  if (evt.isBusy()) {
                      popup.show();
                  }
                  else if (popup.isPopupVisible()) {
                      popup.hide();
                      AdfPage.PAGE.removeBusyStateListener(popup, handleBusyState);
                  }
              }
          }

          function openPopup() {
              var popup = AdfPage.PAGE.findComponent("pt1:closePopup");
              popup.show();
          }

          function closePopup(evt) {
              var popup = AdfPage.PAGE.findComponentByAbsoluteId('page1:closePopup');
              if (popup != null) {
                  popup.hide();
                  AdfPage.PAGE.removeBusyStateListener(popup, closePopup);
              }
          }

          function openPopupMultipleRow() {
              var popup = AdfPage.PAGE.findComponent("page1:closePopupMultipleRow");
              popup.show();
          }

          function openHelpPopup() {
              var popup = AdfPage.PAGE.findComponent("page1:helpPopup");
              popup.show();
          }

          function openSaveChangesPopup() {
              var popup = AdfPage.PAGE.findComponent("page1:confirmPopup");
              popup.show();
          }

          function newWindow() {
              document.getElementById("form1").target = "_blank;targetfeatures=toolbar=no location=no directories=no menubar=no";
          }

          function selfWindow() {
              if (document.getElementById("form1")) {
                  document.getElementById("form1").target = "_self";
              }
          }

          function showGlasspane(event) {
              event.preventUserInput();
          }

          window.onfocus = selfWindow;
          window.onfocus = selfWindow;
