          function openMenu(evt) {
              var menuMob = document.getElementById("pt1:settingsMenu");
              var btnMob = document.getElementById("pt1:settingsBtn");
              var btnClose = document.getElementById("pt1:settingsBtnClose");

              menuMob.classList.toggle("open");
              btnMob.classList.add("d-none");
              btnClose.classList.add("d-flex");
          }

              function closeMenu(evt) {
                  var menuMob = document.getElementById("pt1:settingsMenu");
                  var btnMob = document.getElementById("pt1:settingsBtn");
                  var btnClose = document.getElementById("pt1:settingsBtnClose");

                  menuMob.classList.toggle("open");
                  btnMob.classList.remove("d-none");
                  btnClose.classList.remove("d-flex");
