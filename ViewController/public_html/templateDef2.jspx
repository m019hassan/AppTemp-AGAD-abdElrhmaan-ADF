        <af:resource type="javascript">
          google.load("maps", "3", 
          {
              other_params : "sensor=false"
          });
          // Call this function when the page has been loaded
          var map;
          var marker = null;

          function createMarker(latlng, name, html) {
              var contentString = html;
              marker = new google.maps.Marker( {
                  position : latlng, map : map
              });
              //------------------
              //  AdfPage.PAGE.findComponent("lat").setValue(marker.position.lat()); 
              //    AdfPage.PAGE.findComponent("lng").setValue(marker.position.lng()); 
              //-------------
              $("input[name$=lat]").val(marker.position.lat());
              $("input[name$=lng]").val(marker.position.lng());
              //document.getElementById("pt1:pt_region0:2:lat::content").value = marker.position.lat(); 
              // document.getElementById("pt1:pt_region0:2:lng::content").value = marker.position.lng(); 
              return marker;
          }

          function initialize2(pointsString) {

              //map.setCenter(new google.maps.LatLng(59.32522,18.07002), 13);
              var points = pointsString.split(" ");
              var markers = [];
              var latlngs = [];
              var latt;
              var langg;
              for (i = 0;points.length - 1 &gt; i;i++) {
                  latt = points[i];
                  langg = points[i + 1];
                  latlngs[i] = new google.maps.LatLng(points[i], points[i + 1]);
                  markers[i] = new google.maps.Marker( {
                      position : latlngs[i], map : map
                  });
                  i = i + 1;
              }
              var mapOptions = {
                  center : new google.maps.LatLng(latt, langg), zoom : 18, mapTypeId : google.maps.MapTypeId.HYBRID
              };
              map = new google.maps.Map(document.getElementById("mapdiv2"), mapOptions);

          }

          function initialize(pointsString) {

              //map.setCenter(new google.maps.LatLng(59.32522,18.07002), 13);
              var mapOptions = {
                  center : new google.maps.LatLng(27.5166667, 41.6833333), zoom : 10, mapTypeId : google.maps.MapTypeId.HYBRID
              };
              map = new google.maps.Map(document.getElementById("mapdiv"), mapOptions);

              var latLng = new google.maps.LatLng(27.511620952131192, 41.709938049316406);
              google.maps.event.addListener(map, 'click', function (event) {
                  //call function to create marker
                  if (marker) {
                      marker.setMap(null);
                      marker = null;
                  }
                  marker = createMarker(event.latLng, "name", "Location" + event.latLng);
              });

              var points = pointsString.split(" ");
              var markers = [];
              var latlngs = [];
              for (i = 0;points.length - 1 &gt; i;i++) {
                  latlngs[i] = new google.maps.LatLng(points[i], points[i + 1]);
                  markers[i] = new google.maps.Marker( {
                      position : latlngs[i], map : map
                  });
                  i = i + 1;
              }

          }
