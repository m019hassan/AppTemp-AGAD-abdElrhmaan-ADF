-----------------------------------------*/
af|button
{
  border-radius: 4px !important;
}


af|button::link, af|button, 
{
  background-image: none;
  /*background-color: #3B5DE7;*/
  /*border-color: #3B5DE7 !important;*/
  /*border-radius: .35rem;*/
  color: #fff;
  text-shadow: none;
  transition: all .3s ease-in-out;
  cursor: pointer;
  font-weight: 500;
}

af|button::link
{
  display: flex;
  align-items: center;
  justify-content: center;
  /*height: 18px;
  padding: 5px 5px 4px 5px !important;*/
}

af|button::text
{
  font-weight: 500;
  text-shadow: none;
  margin: 0 0.2rem;
}
af|button img{
    width: 14px;
}


af|button:hover-target,
af|button:focus,
af|button::link:focus,
af|button::link:active,
af|button::link:hover,
af|button::link:hover-target,
af|button::link:depressed,
af|button::link:depressed:hover-target
{
  background-color: transparent !important;
  border-color: transparent  !important;
  cursor: pointer !important;
  font-weight: 500;
  transition: all .3s ease-in-out;
  
}

af|button::text, af|button:hover::link,  af|button:hover-target af|button::text, af|button:depressed:hover-target af|button::text
{
  color: #FFF !important;
}

af|button
{
    background-image: none;
    background-color: #1a5f73 !important;
    border-color: #1a5f73 !important;
    border-radius: 4px;
    color: #fff;
    text-shadow: none;
    transition: all .3s ease-in-out;
    cursor: pointer;
    font-weight: 500;
}
af|button:text-only{
    background-image: none !important;
    background-color: #1a5f73  !important;
    border-color: #1a5f73  !important;
    border-radius: 4px !important;
    color: #fff !important;
    text-shadow: none !important;
    transition: all .3s ease-in-out;
    cursor: pointer;
    font-weight: 500 !important;
    height: 26px;
}

af|button:text-only:hover,
af|button:hover, af|button:hover-target
{
  border-color: #283d92 !important;
  cursor: pointer !important;
  font-weight: 500 !important;
  background-color: #0d3e4d !important;
  color: #FFF !important;
}

af|button.btn::link:before
{
  content: " ";
  display: inline-block;
  width: 24px;
  height: 14px;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
  position: relative;
  top: -1px;
}

af|button.btn-add::link:before
{
  background-image: url("../images/icon/new.svg");
}

af|button.btn-edit::link:before
{
  background-image: url("../images/icon/edit.svg") !important;
}

af|button.btn-save::link:before
{
  background-image: url("../images/icon/save.svg");
}

af|button.btn-search::link:before
{
  background-image: url("../images/icon/search.svg");
}

af|button.btn-del::link, af|button.btn-del
{
  background-color: #e74c3c;
  border-color: #e74c3c !important;
}

af|button.btn-del::link:before
{
  background-image: url("../images/icon/delete.svg");
}

af|button.btn-del:hover-target, af|button.btn-del:focus, af|button.btn-del::link:focus, af|button.btn-del::link:active, af|button.btn-del::link:hover, af|button.btn-del::link:hover-target, af|button.btn-del::link:depressed, af|button.btn-del::link:depressed:hover-target
{
  background-color: #c0392b !important;
  border-color: #c0392b !important;
}
af|button:disabled,
af|button:disabled af|button,
af|button:disabled af|button::link,
af|button:disabled af|button::text,
af|button:disabled af|button::link:focus,
af|button:disabled af|button::link:active,
af|button:disabled af|button::link:hover,
af|button:disabled af|button::link:hover-target,
af|button:disabled af|button:hover-target,
af|button:disabled af|button::link:depressed,
af|button:disabled af|button::link:depressed:hover-target{ 
    background-color: #3b5de7 !important;
    border-color: #3b5de7 !important ;
    cursor: not-allowed;
    color: #FFF !important;
    cursor: not-allowed !important;
    opacity: .65;
}


