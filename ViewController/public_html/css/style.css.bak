/* Menu -- Panel Accordion */
af|panelSplitter.mainPS::horizontal-divider{
    /*display: none;*/
}
af|panelSplitter::{
    width: 0 !important;
    display: none;
}

af|panelAccordion{
    -tr-header-height: 45;
}
af|panelAccordion::header-subsequent:disclosable,
af|panelAccordion::header:disclosable,
af|panelAccordion::header-subsequent:disclosed,
af|panelAccordion::header:disclosed{
    background: #72C18D ; 
    border: 0px;
    border-bottom: 1px solid transparent;
    border-top: 1px solid transparent;
    padding: 0 25px;
    cursor: pointer;
    transition: all 0.3s ease-in-out ;
}
af|panelAccordion::header-subsequent:disclosed:hover,
af|panelAccordion::header:disclosed:hover,
af|panelAccordion::header-subsequent:disclosable:hover,
af|panelAccordion::header:disclosable:hover{
    background: #1a5f73;
}
af|panelAccordion::header-subsequent:disclosed:hover af|panelAccordion::header-title,
af|panelAccordion::header:disclosed:hover af|panelAccordion::header-title,
af|panelAccordion::header-subsequent:disclosable:hover af|panelAccordion::header-title,
af|panelAccordion::header:disclosable:hover af|panelAccordion::header-title{
    color: #fff;
}

af|panelAccordion::header-subsequent:disclosed af|panelAccordion::header-title,
af|panelAccordion::header:disclosed af|panelAccordion::header-title{
    color: #fff ;
    transition: all 0.3s ease-in-out ;
}
af|panelAccordion::header-subsequent:disclosable af|panelAccordion::header-title,
af|panelAccordion::header:disclosable af|panelAccordion::header-title{
    color: #fff ;
    padding-left: 0px;
    transition: all 0.3s ease-in-out ;
}

af|panelAccordion::header-subsequent:disclosable:after,
af|panelAccordion::header:disclosable:after,
af|panelAccordion::header-subsequent:disclosed:after,
af|panelAccordion::header:disclosed:after{
    content: ' ' !important;
    background:  #fff !important;
    width: 3px;
    height: 45px;
    position: absolute;
    top: 0px;
    left: 0px;
}

af|panelAccordion::header-subsequent:disclosable:after:rtl,
af|panelAccordion::header:disclosable:after:rtl,
af|panelAccordion::header-subsequent:disclosed:after:rtl,
af|panelAccordion::header:disclosed:after:rtl{
    left: auto;
    right: 0;
}


af|panelAccordion::header-subsequent:disclosed:before,
af|panelAccordion::header:disclosed:before,
af|panelAccordion::header-subsequent:disclosable:before,
af|panelAccordion::header:disclosable:before{
    /*content: "\e649";
    font-family: themify;*/
    content: "";
    width: 8px;
    height: 8px;
    background-image: url("../images/icon/d-arrow.svg");
    background-size: contain;
    position: absolute;
    top: 50%;
    right: 10px;
    transition: all 0.5s ease-in-out ;
}
af|panelAccordion::header-subsequent:disclosed:before:rtl,
af|panelAccordion::header:disclosed:before:rtl,
af|panelAccordion::header-subsequent:disclosable:before:rtl,
af|panelAccordion::header:disclosable:before:rtl {
    right: auto;
    left: 10px
}

af|panelAccordion::header-subsequent:disclosed:before,
af|panelAccordion::header:disclosed:before{
    transform: rotate(-90deg) translate(-50%, 0);
    right: 10px;
    color: #FFF;
}
af|panelAccordion::header-subsequent:disclosed:before:rtl,
af|panelAccordion::header:disclosed:before:rtl{
    transform: rotate(90deg) translate(-50%, 0);
}
/* this is arrow after title when hover*/
af|panelAccordion::header-subsequent:disclosed:hover:before,
af|panelAccordion::header:disclosed:hover:before,
af|panelAccordion::header-subsequent:disclosable:hover:before,
af|panelAccordion::header:disclosable:hover:before{
    color: #fff;
}
af|panelAccordion::header-title{
    color: #fff;
    text-shadow: none;
    font-size: 14px;
    font-weight: 500;
    padding-left: 0px !important;
    cursor: pointer;
    font-weight: normal;
}
af|panelAccordion::header-subsequent:disclosable af|panelAccordion::header-title,
af|panelAccordion::header-subsequent:disclosed af|panelAccordion::header-title
af|panelAccordion::header:disclosed af|panelAccordion::header-title,
af|panelAccordion::header:disclosable af|panelAccordion::header-title{
    padding-left: 0px;
}

af|panelAccordion::header-subsequent:disclosable:disclosed af|panelAccordion::header-title {
    color: #fff !important;
}

af|panelAccordion::header-disclose {
    display: none;
}

af|panelAccordion::body-content{
    background-color: transparent;
    margin: 0 0 0 2rem;
    border: none;
}
af|panelAccordion::body-content:rtl{
    margin: 0 2rem 0 0;
}
.main-menu af|showDetailItem{
    background-color: transparent !important ;
    background: transparent;
    border: 0px;
}

af|panelAccordion af|link {
    width: calc(100% - 10px);
    padding: 8px 15px 8px 15px;
    font-size: 12px;
    display: block;
    box-sizing: border-box;
    color: #fff;
    transition: all 0.3s ease-in-out;
    background: transparent;
    text-align: left !important;
    border-radius: 0 5rem 5rem 0;
    position: relative;
}
af|panelAccordion af|link:rtl{
    text-align: right !important;
    border-radius: 5rem 0 0 5rem;
}
af|panelAccordion af|link:hover{
    background: rgba(2, 5, 8, 0.3);
}
af|panelAccordion af|link:before{
    content: '';
    width: 2px;
    height: 8px;
    position: absolute;
    margin-top: 0;
    top: 5px;
    left: 0;
    border-radius: 0;
    background: #8e95aa;
}

af|panelAccordion af|link:after{
    content: '';
    width: 2px;
    height: 8px;
    position: absolute;
    margin-top: 0;
    bottom: 5px;
    left: 0;
    border-radius: 0;
    background: #8e95aa;
}
af|panelAccordion af|link:after:rtl,
af|panelAccordion af|link:before:rtl{
    left: auto;
    right: 0;
}
af|panelAccordion af|link::text {
    font-size: 14px;
    text-align: start !important;
    transition: all 0.3s ease-in-out;
}
af|panelAccordion af|link:visited:hover,
af|panelAccordion af|link::text:hover,
af|panelAccordion af|link:hover span,
af|panelAccordion af|link:visited span{
    text-decoration: none !important;
    color: #fff;
    
}
af|panelAccordion af|link:visited:hover,
af|panelAccordion af|link::text:hover{
    color: #fff;
}
