.header {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    /*background-color: #2B8F49;*/
    background-image: url("../images/bg-header.svg");
    background-position: center;
    background-size: cover;
}

.navs{
    display: flex;
    flex-direction: row;
       justify-content: center;
    align-items: center;
    height: 100%;
    margin: 0 1rem;
}

.navs af|link::text {
    color: #eee;
}


.menu-bar
{
  width: 60px;
  height: 50px;
  padding: 0 1rem;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  cursor: pointer;
  background-color: #2b8f49;
}

.menu-bar:hover
{
  background-color: #1A5F73;
}

.menu-bar af|image
{
  width: 100%;
  max-width: 18px;
}

.menu-main-user
{
  height: 50px;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.user-photo
{
  width: 30px;
  max-height: 30px;
  border-radius: 5rem;
}

.user-welcome {
    font-size: 16px;
    font-weight: bold;
    border-bottom: 2px solid #006837;
    color: #006837;
    padding: 0 5px;
    display: inline-block;
}

af|menu.user-menu::bar-item-text{
    color: #006837;
    display: inline-block;
    width: 118px;
    font-size: 14px;
    font-weight: bold;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
af|menu::bar-item:depressed af|menu::bar-item-text{
    color: #FFF !important;
}


.menu-main-user af|menu::bar-item-text
{
  color: #FFF;
}

af|commandMenuItem::menu-item{
    height: 45px;
}

af|commandMenuItem img
{
  width: 16px;
  position: relative;
  top: -1px;
}

.side-menu{
    display: flex;
    flex-direction: column;
    background: #525156;
    height: 100vh;
}
.side-menu .head{
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 0.5rem
}
.side-menu .head .logo{
    width: 100px;
}
.side-menu .head .logo img{
    width: 100%;
}
.side-menu .menu {
	height: calc(100vh - 46px);
    overflow-y: auto;
}
.main-content{
    display: flex;
    flex-direction: column;
    
    background: #fff;
    justify-content: space-between;
}
.content-wrapper {
    display: block;
    height: calc(100vh - 80px) !important;
    padding: 0.5rem 1rem 1rem; 
    overflow: auto;
    box-sizing: border-box;
}
.footer{
    display: flex;
    height: 30px;
    justify-content: center;
    align-items: center;
    background: #2b8f49;
    color: #fff;
    /*padding: 0.5rem 0;*/
}
.form-groupe{
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: start;
}


.form-groupe af|outputLabel{
    display: inline-block;
    margin-bottom: 0.5rem;
    line-height: normal;
}
.form-groupe af|inputTextarea,
.form-groupe af|inputComboboxListOfValues,
.form-groupe af|inputListOfValues,
.form-groupe af|inputColor,
.form-groupe af|inputDate,
.form-groupe af|selectOneChoice,
.form-groupe textarea,
.form-groupe af|inputText{
    margin-bottom: 1rem;
    width: 100%
}
